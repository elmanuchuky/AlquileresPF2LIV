<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewClient</servlet-name>
        <servlet-class>Servlets.NewClient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewRental</servlet-name>
        <servlet-class>Servlets.NewRental</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AlterRental</servlet-name>
        <servlet-class>Servlets.AlterRental</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientsDetails</servlet-name>
        <servlet-class>Servlets.ClientsDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentalsList</servlet-name>
        <servlet-class>Servlets.RentalsList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GatheredByFloor</servlet-name>
        <servlet-class>Servlets.GatheredByFloor</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StallsList</servlet-name>
        <servlet-class>Servlets.StallsList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StallsAvailable</servlet-name>
        <servlet-class>Servlets.StallsAvailable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Index</servlet-name>
        <servlet-class>Servlets.Index</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewClient</servlet-name>
        <url-pattern>/NewClient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewRental</servlet-name>
        <url-pattern>/NewRental</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AlterRental</servlet-name>
        <url-pattern>/AlterRental</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientsDetails</servlet-name>
        <url-pattern>/ClientsDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentalsList</servlet-name>
        <url-pattern>/RentalsList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GatheredByFloor</servlet-name>
        <url-pattern>/GatheredByFloor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StallsList</servlet-name>
        <url-pattern>/StallsList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StallsAvailable</servlet-name>
        <url-pattern>/StallsAvailable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>first.jsp</welcome-file>
    </welcome-file-list>
</web-app>
